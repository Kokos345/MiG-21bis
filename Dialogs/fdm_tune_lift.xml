<PropertyList>

 <name>fdm_tuner</name>
 <modal>false</modal>
 <layout>vbox</layout>
 <width>450</width>
 <height>300</height>
  <group>
   <text>
    <label>Tune dat lift</label>
	<x>10</x>
	<y>275</y>
  </text>
  
      <!-- slider 1 -->
      
      <text>
         <x>10</x>
         <y></y>
         <label>Lift_alpha</label>
      </text>
      
      <text>
         <x>245</x>
         <y></y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/lift/alpha</property>
      </text>
      
      <text>
         <x>400</x>
         <y></y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/force/Lift_alpha</property>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>210</y>
         <width>425</width>
         <height>30</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/lift/alpha</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 2 -->
      
      <text>
         <x>10</x>
         <y></y>
         <label>Lift_pitch_rate</label>
      </text>
      
      <text>
         <x>245</x>
         <y></y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/lift/pitch_rate</property>
      </text>
      
      <text>
         <x>400</x>
         <y></y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/force/Lift_pitch_rate</property>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>210</y>
         <width>425</width>
         <height>30</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/lift/pitch_rate</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 3 -->
      
      <text>
         <x>10</x>
         <y></y>
         <label>Lift_alpha_rate</label>
      </text>
      
      <text>
         <x>245</x>
         <y></y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/lift/alpha_rate</property>
      </text>
      
      <text>
         <x>400</x>
         <y></y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/force/Lift_alpha_rate</property>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>210</y>
         <width>425</width>
         <height>30</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/lift/alpha_rate</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 4 -->
      
      <text>
         <x>10</x>
         <y></y>
         <label>Lift_elevator</label>
      </text>
      
      <text>
         <x>245</x>
         <y></y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/lift/elevator</property>
      </text>
      
      <text>
         <x>400</x>
