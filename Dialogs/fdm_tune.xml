<PropertyList>

 <name>fdm_tuner</name>
 <modal>false</modal>
 <layout>vbox</layout>
 <width>450</width>
 <height>300</height>
  <group>
   <text>
    <label>Tune dat fdm</label>
	<x>10</x>
	<y>275</y>
  </text>
  
  <slider>
   <vertical>false</vertical>
   <x>10</x>
   <y>50</y>
   <width>425</width>
   <property>/fdm/jsbsim/tune/five</property>
   <min>-2</min>
   <max>2</max>
    <binding>
     <command>dialog-apply</command>
    </binding>
  </slider>

    <slider>
   <vertical>false</vertical>
   <x>10</x>
   <y>90</y>
   <width>425</width>
   <property>/fdm/jsbsim/tune/four</property>
   <min>-20</min>
   <max>20</max>
    <binding>
     <command>dialog-apply</command>
    </binding>
  </slider>

    <slider>
   <vertical>false</vertical>
   <x>10</x>
   <y>130</y>
   <width>425</width>
   <property>/fdm/jsbsim/tune/three</property>
   <min>-20</min>
   <max>20</max>
    <binding>
     <command>dialog-apply</command>
    </binding>
  </slider>

    <slider>
   <vertical>false</vertical>
   <x>10</x>
   <y>170</y>
   <width>425</width>
   <property>/fdm/jsbsim/tune/two</property>
   <min>-2</min>
   <max>2</max>
    <binding>
     <command>dialog-apply</command>
    </binding>
  </slider>

    <slider>
   <vertical>false</vertical>
   <x>10</x>
   <y>210</y>
   <width>425</width>
   <property>/fdm/jsbsim/tune/one</property>
   <min>-2</min>
   <max>2</max>
    <binding>
     <command>dialog-apply</command>
    </binding>
  </slider>
  
  <button>
    <legend>Apply</legend>
	<y>10</y>
	<x>10</x>
    <binding>
     <command>dialog-apply</command>
    </binding>
   </button>
  
  <button>
    <legend>Close</legend>
	<y>10</y>
	<x>167</x>
    <binding>
     <command>dialog-apply</command>
    </binding>
	<binding>
     <command>dialog-close</command>
    </binding>
   </button>
  
  </group>

</PropertyList>